<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.WhatToEat" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/dark_blue_grad</item>
        <item name="colorPrimaryVariant">@color/brown</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/blue</item> <!-- List view selected text color -->mm
        <item name="colorSecondaryVariant">@color/dark_green_yellow</item> <!-- List view selected background and divider color -->
        <item name="colorOnSecondary">@color/black</item> <!-- List view selected background and divider -->
        <!-- Status bar color. -->
        <item name="android:statusBarColor" >@color/dark_blue</item>
        <item name="android:windowLightStatusBar">true</item>
        <!-- Customize your theme here. -->
    </style>
</resources>